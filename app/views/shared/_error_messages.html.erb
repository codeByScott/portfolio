<% if object.errors.any? %>
  <div id="error_explanation">
    <div class="alert callout">
      This form has <%= pluralize(object.errors.count, "error") %>:
      <ul>
        <% object.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
      </ul>
    </div>
   </div> 
<% end %>